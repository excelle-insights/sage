{
    "name": "excelle-insights/sage",
    "description": "Sage Online integration package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/process": "^7.0",
        "robmorgan/phinx": "^0.16.9",
        "sage/v3-php-sdk": "^6.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "ExcelleInsights\\Sage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExcelleInsights\\Sage\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}